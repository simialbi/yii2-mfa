{
    "name": "simialbi/yii2-mfa",
    "type": "yii2-extension",
    "description": "Multi factor authentication for yii2-framework",
    "keywords": [
        "yii2",
        "mfa",
        "Multi factor authentication",
        "totp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "simialbi",
            "email": "simi.albi@outlook.com"
        }
    ],
    "support": {
        "source": "https://github.com/simialbi/yii2-ews",
        "issues": "https://github.com/simialbi/yii2-ews/issues"
    },
    "minimum-stability": "beta",
    "require": {
        "php": ">=8.1",
        "endroid/qr-code": "^4.8.2",
        "robthree/twofactorauth": "^2.0.0",
        "yiisoft/yii2": "^2.0.20"
    },
    "require-dev": {
        "yiisoft/yii2-bootstrap5": "^2.0.0",
        "yiisoft/yii2-coding-standards": "~2.0",
        "phpunit/phpunit": "^9.5.21"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "To use with bootstrap 3",
        "yiisoft/yii2-bootstrap4": "To use with bootstrap 4",
        "yiisoft/yii2-bootstrap5": "To use with bootstrap 5"
    },
    "autoload": {
        "psr-4": {
            "simialbi\\yii2\\mfa\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
